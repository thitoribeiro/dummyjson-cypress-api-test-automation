  "scripts": {
    "test:headless": "npx cypress run",
    "test:ui": "npx cypress open",
    "test:docker": "docker-compose up --build",
    "test:docker:one": "docker-compose run tests npx cypress run --spec",
    "test:docker:one:report": "npm run test:docker:one --",
    "report:open": "open mochawesome-report/index.html",
    "test:docker:report": "npm run test:docker && npm run report:open",
